# App
PORT=3000
NODE_ENV=development
SESSION_SECRET=change-me-to-a-strong-secret

# MongoDB
# Local dev:
MONGO_URI=mongodb://127.0.0.1:27017/auth_webapp
# Docker Compose override example:
# MONGO_URI=mongodb://mongo:27017/auth_webapp

# Startup retry settings (useful in container environments)
MONGO_RETRY_ATTEMPTS=10
MONGO_RETRY_DELAY_MS=3000
